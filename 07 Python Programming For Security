datetime_module.py

# Wersja z prostym importem
import datetime

def Clock():
    now = datetime.datetime.today()
    print("hour: ", now.hour)
    print("minute: ", now.minute)
    print("second: ", now.second)

Clock()

# Wersja z from
from datetime import datetime 

def Clock():
    now = datetime.today()
    print("hour: ", now.hour)
    print("minute: ", now.minute)
    print("second: ", now.second)

Clock()

#Wersja z from i as
from datetime import datetime as dt

def Clock():
    now = dt.today()
    print("hour: ", now.hour)
    print("minute: ", now.minute)
    print("second: ", now.second)

Clock()

# Wersja z as
import datetime as dt 

def Clock():
    now = dt.datetime.today()
    print("hour: ", now.hour)
    print("minute: ", now.minute)
    print("second: ", now.second)

