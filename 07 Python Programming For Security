loopsandlists.py (2 listy)

colors = ["blue", "red", "green", "pink", "purple", "yellow", "brown"]
fruits = ["bananas", "apples", "oranges", "melons"]


for color in colors:
    for fruit in fruits:
        if len(color) == len(fruit):
            print(color, "i", fruit, "majÄ… takÄ… samÄ… dlugoÅ›Ä‡")
        elif fruit[0] == color[0]:
            print(color, "i", fruit, "majÄ… takÄ… samÄ… pierwszÄ… literÄ™")
        elif len(fruit) > 5 and len(color) > 5:
            print(f'sÅ‚owa {color} i {fruit} sÄ… dÅ‚ugie!')
        elif len(fruit) > 6 or len(color) == 6:
            print(f'sÅ‚owo {color} lub {fruit} jest dÅ‚ugie!')

