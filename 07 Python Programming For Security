mathematic.py

number_a = 7
number_b = 5

#Dodawanie
print(number_a + number_b)
print(type(number_a + number_b))

#Odejmowanie
print(number_a - number_b)
print(type(number_a - number_b))

#MnoÅ¼enie
print(number_a * number_b)
print(type(number_a * number_b))

#Dzielenie
print(number_a / number_b)
print(type(number_a / number_b))

#PotÄ™ga
print(number_a ** number_b)
print(type(number_a ** number_b))

#Dzielenie z zaokrÄ…gleniem "podÅ‚oga"
print(number_a // number_b)
print(type(number_a // number_b))

#Modulo
print(number_a % number_b)
print(type(number_a % number_b))

#Modulo wÅ‚asnorÄ™cznie
print(number_a - (number_a // number_b) * number_b)

