scope.py

numberA = 6
numberB = 15

print(numberA)
print(numberB)

print("-----------")

def Local():
    numberA = 8
    print(numberA)

def Global():
    global numberB
    numberB = 8
    print(numberB)

Local()
Global()

print("-----------")

print(numberA)
print(numberB)

