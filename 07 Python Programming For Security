### ğŸ“‹ â€“ Lab07 Hot N Cold

import random

number_of_tries = 0
random_number = 0

def check_user_prediction(userPrediction):
    global random_number, number_of_tries

    number_of_tries += 1
    if userPrediction == random_number:
        print("Brawo! ZgadÅ‚eÅ›")
        return True
    elif abs(userPrediction - random_number) <= 5:
        print("Bardzo gorÄ…co")
        return False
    elif abs(userPrediction - random_number) <= 10:
        print("GorÄ…co")
        return False
    elif abs(userPrediction - random_number) <= 20:
        print("Letnio")
        return False
    elif abs(userPrediction - random_number) <= 40:
        print("Zimno")
        return False
    else:
        print("Bardzo zimno")
        return False

def definerandomnumber():
    global random_number
    random_number = random.randint(1, 100)

def main():
    global number_of_tries
    definerandomnumber()

    print("Witaj w programie!")
    print("WylosowaÅ‚em liczbÄ™ od 1 do 100. SprÃ³buj jÄ… zgadnÄ…Ä‡!")

    isFind = False
    while isFind == False:
        userPrediction = int(input("Podaj liczbÄ™: "))
        isFind = check_user_prediction(userPrediction)

    print("liczba wykorzystanych prÃ³b: ", number_of_tries)


if __name__ == "__main__":
    main()

