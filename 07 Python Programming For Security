serversocketlargedata.py

import socket

data = b""
buffer = 2048

mysocket = socket.socket()
mysocket.bind(("0.0.0.0", 4444))
mysocket.listen(1)
conn, addr = mysocket.accept()

while True:
    packet = conn.recv(buffer)
    print(len(packet))
    data += packet
    if len(packet) < buffer:
        print("All data!")
        break
mysocket.close()

with open("output.txt", "wb") as f:
    f.write(data)

