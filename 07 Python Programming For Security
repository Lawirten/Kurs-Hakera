whileloop.py

name = "Mateusz"

print("Start")

while len(name) < 60:
    print(len(name))
    name *= 2
    print(len(name))
    print("-------------")

print("Koniec")

***

whileloop.py (z warunkiem)

# Z continue

while len(name) < 60:
    name *= 2
    print(name)
    print(name.count("M"))
    if name.count("M") == 4:
        continue
    print("-------------")

print("Koniec")

# Z break

while len(name) < 60:
    name *= 2
    print(name)
    print(name.count("M"))
    if name.count("M") == 4:
        break
    print("-------------")

print("Koniec")

