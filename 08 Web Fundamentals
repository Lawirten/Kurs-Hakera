***

Operatory porÃ³wnawcze

<?php
  $equal = 5 == "5" ; //true
  $identical = 5 === "5" ; //false
  $not_equal = 5 != 3 ; //true
  $not_identical = 5 !== "5" ; //true
  $greater = 5 > 3 ; //true
  $great_eq = 5 >= 3; //true
  $lesser =  3 < 2 ; //false
  $less_eq = 5 <= 5 ; //true
  
  echo "$equal <br>";
  echo "$identical <br>";
  echo "$not_equal <br>";
  echo "$not_identical <br>";
  echo "$greater <br>";
  echo "$great_eq <br>";
  echo "$lesser <br>";
  echo "$less_eq <br>";
  ?>
  
  ***
  
  Instrukcje warunkowe
    
<?php
  if (5 > 2){
    echo("5 jest wieksze od 2.");
    } elseif (5 === 9){
      echo("5 jest identyczne z 9.");
    } else {
      echo("Wszystko to nie jest prawda.");
    }
  ?>
  
  ***
  
  Operatory logiczne
    
<?php
  if (5 > 2 and 6 > 3){
    echo("5 jest wieksze od 2 i 6 jest wieksze od 3.");
    } elseif (5 === 3 or 1 > 2){
      echo("5 jest identyczne z 9 albo 1 jest wieksze od 2.");
    }elseif (3 > 2 xor 2 < 1){
      echo("Jedno jest poprawne, ale drugie nie.");
    } else {
      echo("Wszystko to nie jest prawda.");
    }
  ?>
  
  
  ***
  
  Funkcje
  
  
<?php
  $val1 = 1;
  $val2 = 2;
  
  function mYFunc(){
    echo "cos";
  }

  function mYFunc2($val1, $val2){
    echo $val1, $val2;
  }
  function mYFunc3(){
    return "value";
  }
  mYFunc();
  mYFunc2($val1, $val2);
  mYFunc3();
  ?>
  
