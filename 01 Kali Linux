Dowiązanie symboliczne
# utworzenie pliku
touch plik1
cat plik1
echo "text" > plik1
cat plik1

# dowiązanie
ln -s plik1 plik2
ls -lah
# dodanie kolejnej frazy do pliku1
echo "text2" >> plik1
# wyświetlenie - zawartość jest identyczna
cat plik1
cat plik2

